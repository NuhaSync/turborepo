{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".next/**"]
		},
		"dev": {
			"dependsOn": ["^dev"],
			"cache": false
		},
		"check": {
			"dependsOn": ["^check"],
			"outputs": []
		}
	}
}
